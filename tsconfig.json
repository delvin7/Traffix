{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",       // Add this line to include .jsx files
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/index.jsx"
, "tailwind.config.js"  ]
}
